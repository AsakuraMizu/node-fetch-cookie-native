{
  "name": "node-fetch-cookie-native",
  "version": "0.0.2",
  "type": "module",
  "types": "./index.d.ts",
  "module": "./dist/node.js",
  "exports": {
    ".": {
      "browser": {
        "import": {
          "types": "./index.d.ts",
          "default": "./dist/browser.js"
        },
        "require": {
          "types": "./index.d.cts",
          "default": "./dist/browser.js"
        }
      },
      "node": {
        "import": {
          "types": "./index.d.ts",
          "default": "./dist/node.js"
        },
        "require": {
          "types": "./index.d.cts",
          "default": "./dist/node.cjs"
        }
      },
      "import": {
        "types": "./index.d.ts",
        "default": "./dist/node.js"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./dist/node.cjs"
      }
    }
  },
  "files": [
    "dist",
    "index.d.ts",
    "index.d.cts"
  ],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "fetch-cookie": "*",
    "node-fetch-native": "*"
  },
  "devDependencies": {
    "@types/tough-cookie": "^4.0.3",
    "tsup": "^7.2.0"
  }
}
